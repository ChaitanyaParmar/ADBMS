1) CREATE USER Pratik IDENTIFIED BT ptk123;
   CREATE USER Deepen IDENTIFIED BT deepen123;
2) GRANT CREATE SESSION TO Pratk;
   GRANT CREATE SESSION TO Deepen;
3) GRANT SELECT ON student TO Pratik;
4) GRANT ALL ON student TO Deepen;
5) REVOKE ALL ON student FROM Pratik;
6) REVOKE DELETE ON student FROM Deepen;
